  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "target": "es2022",
    "lib": ["es2023", "DOM", "DOM.Iterable"],
    "module": "preserve",
    "strict": true,
    "skipLibCheck": true,
    "outDir": "build",
    "noEmit": true,
    "allowImportingTsExtensions": true
  }
}
